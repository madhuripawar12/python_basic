print("Enter Marks Of 5 Subjects out of 100: ")
l=[]
total=0
for i in range(5):
    num=input("Enter Marks: ")
    l.append(num)
for i in l:
    total+=i
perc=(float(total)/500)*100
print perc,total
if perc>=80:
    print "Grade A"
elif 60<=perc<80:
    print "Grade B"
elif 40<perc<60:
    print "Pass"
else:
    print "Fail"
